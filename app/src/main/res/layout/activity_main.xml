<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.iMightBeIndian.PongOnParticlePhoton.MainActivity">


    <FrameLayout
        android:id="@+id/fragmentbtn"
        android:name="com.example.svettleif.tictactoe.ButtonFragment"
        android:layout_width="334dp"
        android:layout_height="372dp"
        android:layout_marginBottom="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout="@layout/button_fragment" />

    <FrameLayout
        android:id="@+id/newFragment"
        android:name="com.example.svettleif.tictactoe.topFragment"
        android:layout_width="498dp"
        android:layout_height="237dp"
        android:layout_marginBottom="53dp"
        android:layout_marginTop="28dp"
        app:layout_constraintBottom_toTopOf="@+id/fragmentbtn"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.333"
        tools:layout="@layout/top_fragment" />


</android.support.constraint.ConstraintLayout>